      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7FDF7"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCOR9TirjkqbmxLSuwTtJrCIK1nAyR2IZQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "BASE_URL": "https://trodden.herokuapp.com",
      "CHAT_URL": "https://trodden-chat.herokuapp.com",
      "PLACE_API_KEY": "AIzaSyCOR9TirjkqbmxLSuwTtJrCIK1nAyR2IZQ"
